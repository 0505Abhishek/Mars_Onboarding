<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="Content-Language" content="en">
    <title>Admin-MARS</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    
    <link rel="stylesheet" href="../images/ficon (1).png">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css">
    
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@mdi/font/css/materialdesignicons.min.css">  

    <link rel="stylesheet" href="../css/style.css"> 

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@mdi/font/css/materialdesignicons.min.css">  
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

<style>
  .menu-icon {
    font-size: 24px; 
}

.logo-src1{
  font-family: Arial, sans-serif;
  font-size: 50px;
  font-weight: bold;
  color: white;
  padding: 10px 20px;
  display: inline-block;
  border-radius: 5px;
}
.app-header__content{
  background-color: #0000A0; 
}
.app-header__content {
    display: flex;
    align-items: center;
    justify-content: flex-end;
    padding-right: 20px;
}

.app-header__content i {
    font-size: 24px;
    color: white;
    cursor: pointer;
}

</style>
<body id="element">
    <div class="app-container app-theme-white body-tabs-shadow fixed-sidebar fixed-header">
      <div class="app-header header-shadow">
        <!-- Logo and Sidebar Toggle -->
        <div class="app-header__logo text-center">
            <div class="logo-src1">MARS</div>
            <div class="header__pane ml-auto">
                <button type="button" class="hamburger close-sidebar-btn hamburger--elastic" data-class="closed-sidebar">
                    <span class="hamburger-box">
                        <span class="hamburger-inner"></span>
                    </span>
                </button>
            </div>
        </div>
    
        <!-- Mobile Menu Toggle -->
        <div class="app-header__mobile-menu">
            <button type="button" class="hamburger hamburger--elastic mobile-toggle-nav">
                <span class="hamburger-box">
                    <span class="hamburger-inner"></span>
                </span>
            </button>
        </div>
    
        <!-- Header Menu Toggle -->
        <div class="app-header__menu">
            <button type="button" class="btn-icon btn-icon-only btn btn-primary custom-btn btn-sm mobile-toggle-header-nav">
                <span class="btn-icon-wrapper">
                    <i class="fa fa-ellipsis-v"></i>
                </span>
            </button>
        </div>
    
        <!-- Header Content -->
        <div class="app-header__content">
          <i class="fa-regular fa-circle-user" style="font-size: 24px; color: white;"></i>
      </div>
      
      </div>
    
        <div class="app-main">
            <div class="app-sidebar sidebar-shadow">
                <div class="app-header__logo">
                    <div class="logo-src"></div>
                    <div class="header__pane ml-auto">
                        <div>
                            <button type="button" class="hamburger close-sidebar-btn hamburger--elastic"
                                data-class="closed-sidebar">
                                <span class="hamburger-box">
                                    <span class="hamburger-inner"></span>
                                </span>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="app-header__mobile-menu">
                    <div>
                        <button type="button" class="hamburger hamburger--elastic mobile-toggle-nav">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
                <div class="app-header__menu">
                    <span>
                        <button type="button"
                            class="btn-icon btn-icon-only btn btn-primary custom-btnbtn-sm mobile-toggle-header-nav">
                            <span class="btn-icon-wrapper">
                                <i class="fa fa-ellipsis-v fa-w-6"></i>
                            </span>
                        </button>
                    </span>
                </div>
                <div class="scrollbar-sidebar">
                  <div class="app-sidebar__inner">
                    <ul class="vertical-nav-menu">
                        <% token.forEach(function(item) { %>
                            <% if (item.view === 1) { %> 
                                <li class="nav-item">
                                    <% if (item.module_name === 'DASHBOARD') { %>
                                        <a class="nav-link" href="/dashboard">
                                            <i class="mdi mdi-home menu-icon"></i>
                                            <span class="menu-title">DASHBOARD</span>
                                        </a>
                                    
                                    <% } else if (item.module_name === 'PROSPECTIVE_DISTRIBUTOR') { %>
                                        <a class="nav-link" href="/Add_distributor">
                                            <i class="mdi mdi-contacts menu-icon"></i>
                                            <span class="menu-title">Lead Generation</span>
                                        </a>
                                    <% } else if (item.module_name === 'APPROVER_LEAD') { %>
                                        <a class="nav-link" href="/approve_lead">
                                            <i class="fa-solid fa-file-circle-check"></i>
                                            <span class="menu-title">Approve Lead</span>
                                        </a>   
                                    <% } else if (item.module_name === 'LEAD_STATUS') { %>
                                        <a class="nav-link" href="/lead_status">
                                            <i class="fa-solid fa-list-check"></i>
                                            <span class="menu-title">Lead Status</span>
                                        </a>      
                                    <% } else if (item.module_name === 'INVITE') { %>
                                        <a class="nav-link" href="/invite">
                                            <i class="mdi mdi-content-save-all menu-icon"></i>
                                            <span class="menu-title">Invite</span>
                                        </a>                                  
                                    <% } else if (item.module_name === 'APPROVERINDOX') { %>
                                        <a class="nav-link" href="/approver_indox">
                                            <i class="fa-solid fa-envelope-circle-check"></i>
                                            <span class="menu-title">Approval Inbox</span>
                                        </a>
                                    
                                    <% } else if (item.module_name === 'REPORT') { %>
                                        <a class="nav-link" href="/report">
                                            <i class="fa-solid fa-file-lines"></i>
                                            <span class="menu-title">Report</span>
                                        </a>
                                    <% } else if (item.module_name === 'DISTRIBUTORS_LIST') { %>
                                        <a class="nav-link" href="/distributorList">
                                            <i class="fa-solid fa-file-contract"></i>
                                            <span class="menu-title">Document Verification</span>
                                        </a>
                                    
                                    <% } else if (item.module_name === 'ROLE_MANAGEMENT') { %>
                                        <a class="nav-link" href="/role_managment">
                                            <i class="mdi mdi-roller-skate menu-icon"></i>
                                            <span class="menu-title">Role Management</span>
                                        </a>

                                    <% } %>
                                </li>
                            <% } %>
                        <% }); %>
                
                        <!-- Logout Button -->
                        <li class="nav-item">
                            <a class="nav-link" href="/account">
                                <i class="fa-solid fa-right-from-bracket"></i>
                                <span class="menu-title">LOGOUT</span>
                            </a>
                        </li>
                    </ul>
                </div>
                
                </div>
            </div>
            <div class="app-main__outer">
                <div class="app-main__inner">